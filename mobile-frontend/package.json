    "axios": "^1.6.5",
    "expo": "^52.0.46",
    "react": "19.0.0",
    "react-dom": "^19.1.0",
    "react-native": "0.79.1",
    "react-native-chart-kit": "^6.12.0",
    "react-native-responsive-dimensions": "^3.1.1",
